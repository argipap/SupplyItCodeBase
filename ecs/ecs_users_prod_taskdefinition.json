{
  "containerDefinitions": [
    {
      "name": "supplyit-users",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users:production",
            "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-users-prod",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "environment": [
        {
          "name": "APP_SETTINGS",
          "value": "project.config.ProductionConfig"
        },
        {
          "name": "DATABASE_TEST_URL",
          "value": "postgres://postgres:postgres@supplyit-users-db:5432/users_test"
        },
        {
          "name": "DATABASE_URL",
          "value": "%s"
        },
        {
          "name": "SECRET_KEY",
          "value": "%s"
        }
      ],
      "memoryReservation": 200,
      "essential": true
    },
    {
      "name": "redis",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-redis:production",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-redis-prod",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 6379
        }
      ],
      "memoryReservation": 100,
      "essential": true
    },
    {
      "name": "celery",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users:production",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-celery-prod",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [],
      "command": [
        "celery",
        "-A",
        "project.utils.tasks",
        "worker",
        "--loglevel=info"
      ],
      "environment": [
        {
          "name": "CELERY_BACKEND",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "CELERY_BROKER",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "MAILGUN_API_KEY",
          "value": "%s"
        },
        {
          "name": "MAILGUN_DOMAIN",
          "value": "%s"
        },
        {
          "name": "FROM_TITLE",
          "value": "SUPPLYIT"
        }
      ],
      "workingDirectory": "/usr/src/app",
      "memoryReservation": 100,
      "essential": true,
      "links": [
        "supplyit-users",
        "redis"
      ]
    },
    {
      "name": "celery-monitor",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users:production",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-celery-monitor-prod",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 5555,
          "protocol": "tcp",
          "containerPort": 5555
        }
      ],
      "command": [
        "flower",
        "-A",
        "project.utils.tasks",
        "--port=5555",
        "--broker=redis://redis:6379/0"
      ],
      "workingDirectory": "/usr/src/app",
      "memoryReservation": 100,
      "essential": true,
      "links": [
        "supplyit-users",
        "redis"
      ]
    }
  ],
  "family": "supplyit-users-prod-td"
}