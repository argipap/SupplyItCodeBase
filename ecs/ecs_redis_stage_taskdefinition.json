{
  "containerDefinitions": [
    {
      "name": "redis",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-redis:staging",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 6379
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-redis-stage",
          "awslogs-region": "us-east-1"
        }
      }
    }
  ],
  "family": "supplyit-redis-stage-td"
}