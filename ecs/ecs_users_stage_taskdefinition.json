{
  "containerDefinitions": [
    {
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users:staging",
      "name": "supplyit-users",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-users-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5000
        }
      ],
      "environment": [
        {
          "name": "APP_SETTINGS",
          "value": "project.config.StagingConfig"
        },
        {
          "name": "CELERY_BACKEND",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "CELERY_BROKER",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "DATABASE_TEST_URL",
          "value": "postgres://postgres:postgres@supplyit-users-db:5432/users_test"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgres://postgres:postgres@supplyit-users-db:5432/users_stage"
        },
        {
          "name": "SECRET_KEY",
          "value": "my_precious"
        }
      ],
      "memoryReservation": 200,
      "essential": true,
      "links": [
        "supplyit-users-db",
        "redis"
      ]
    },
    {
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users-db:staging",
      "name": "supplyit-users-db",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-users_db-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 5432
        }
      ],
      "environment": [
        {
          "name": "POSTGRES_PASSWORD",
          "value": "postgres"
        },
        {
          "name": "POSTGRES_USER",
          "value": "postgres"
        }
      ],
      "memoryReservation": 200
    },
    {
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-redis:staging",
      "name": "redis",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-redis-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 6379
        }
      ],
      "memoryReservation": 100
    },
    {
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/supplyit-users:staging",
      "name": "celery",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "supplyit-celery-stage",
          "awslogs-region": "us-east-1"
        }
      },
      "command": [
        "celery",
        "-A",
        "project.utils.tasks",
        "worker",
        "--loglevel=info"
      ],
      "environment": [
        {
          "name": "CELERY_BACKEND",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "CELERY_BROKER",
          "value": "redis://redis:6379/0"
        },
        {
          "name": "MAILGUN_API_KEY",
          "value": "%s"
        },
        {
          "name": "MAILGUN_DOMAIN",
          "value": "%s"
        },
        {
          "name": "FROM_TITLE",
          "value": "SUPPLYIT"
        }
      ],
      "workingDirectory": "/usr/src/app",
      "memoryReservation": 130,
      "essential": true,
      "links": [
        "supplyit-users",
        "redis"
      ]
    }
  ],
  "family": "supplyit-users-stage-td"
}